package com.innovect.com.innovect.interfacePractise;

public class Frequency {
  char result ;
static final int SIZE =100;
  String calculateFrequency(String text) {
    int count = 0, reps = 0;
   char string[] = text.toCharArray();
    int n = text.length();
    String arr[] = new String[SIZE];
    for (int i = 0; i < n; i++) {
      arr[i] = text;
    }
    for (int j = 0; j < n; j++) {
      for (int k = 0; k < j; k++) {
        if (string[k] == string[j])
          count++;
        if (count > reps) {
          reps = count;
          result = string[j];
        }
      }
      String str = new String(string);
    }
    return str;
  }


  public static void main(String[] args) {
    Frequency frequency = new Frequency();
    String text1 = "a s d f a d f g a f a f a a";
    String op = frequency.calculateFrequency(text1);
      System.out.println(op);
  }
}
